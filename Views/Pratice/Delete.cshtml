@model CoreLayout.Models.Masters.PraticeModel
@{
    ViewData["Title"] = "Delete";
}
<br />
<h2 class="text-info">Delete Pratice</h2>
<form asp-action="Delete" method="get">
    <div class="pd-4 rounded border">
        <input type="hidden" asp-for="Id" />
        <div asp-validation-summary="ModelOnly" class="text-danger">

        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Name"> </label>
            </div>
            <div class="col-5">
                <input asp-for="Name" readonly="readonly" class="form-control" />
            </div>
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="CountryId"> </label>
            </div>
            <div class="col-5">
                <input asp-for="CountryId" readonly="readonly" class="form-control" />
            </div>
            <span asp-validation-for="CountryId" class="text-danger"></span>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="StateId"> </label>
            </div>
            <div class="col-5">
                <input asp-for="StateId" readonly="readonly" class="form-control" />
            </div>
            <span asp-validation-for="StateId" class="text-danger"></span>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="CityId"> </label>
            </div>
            <div class="col-5">
                <input asp-for="CityId" readonly="readonly" class="form-control" />
            </div>
            <span asp-validation-for="CityId" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="roleid"> </label>
            </div>
            <div class="col-5">
                <input asp-for="roleid" readonly="readonly" class="form-control" />
            </div>
            <span asp-validation-for="roleid" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" asp-route-id="@Model.Id" class="btn btn-danger" value="Delete" />
            <a asp-action="Index" class="btn btn-success">Back to List</a>
        </div>
</form>
